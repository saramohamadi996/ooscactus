{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "cyaxaress/common": "dev-master",
        "cyaxaress/front": "dev-master",
        "cyaxaress/payment": "dev-master",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "kavenegar/laravel-notification": "^1.0",
        "laravel-notification-channels/telegram": "^0.6.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "morilog/jalali": "3.*",
        "spatie/laravel-permission": "^3.13"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "modules/Cyaxaress/Common"
        },
        {
            "type": "path",
            "url": "modules/Cyaxaress/Front"
        },
        {
            "type": "path",
            "url": "modules/Cyaxaress/Payment"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Cyaxaress\\User\\" : "modules/Cyaxaress/User/",
            "Cyaxaress\\Dashboard\\" : "modules/Cyaxaress/Dashboard/",
            "Cyaxaress\\Category\\" : "modules/Cyaxaress/Category/",
            "Cyaxaress\\RolePermissions\\" : "modules/Cyaxaress/RolePermissions/",
            "Cyaxaress\\Course\\" : "modules/Cyaxaress/Course/",
            "Cyaxaress\\Media\\" : "modules/Cyaxaress/Media/",
            "Cyaxaress\\Discount\\" : "modules/Cyaxaress/Discount/src",
            "Cyaxaress\\Ticket\\" : "modules/Cyaxaress/Tickets/src",
            "Cyaxaress\\Comment\\" : "modules/Cyaxaress/Comment/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
